<div class="container">
  <h1>Found a pet ?</h1>
  <%= simple_form_for(@pet) do |f| %>
    <%= f.input :name %>
    <%= f.input :species, collection: %w(dog cat monkey rabbit) %>
    <%= f.input :location_found %>
    <%= f.input :found_at %>
    <%= f.button :submit, class: "btn btn-primary" %>
  <% end %>
</div>
